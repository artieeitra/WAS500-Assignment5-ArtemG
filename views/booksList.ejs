<html>
    <head>
        <title>Booklist</title>
        <style type="text/css">
            #side-bar {
                height: 175%; width: 150px; position: absolute; left: 0; right: 0; padding-left:5px; padding-top: 20px; background-color: rgb(163, 155, 155);
            }
            ul {
                list-style:none; margin:0px; padding:0px; width:50%;
            }
            a {
                text-decoration: none; color: black; font-size: 16px;
            }
            .main  {
            margin-left: 150px;
            }
        </style>
    </head>
    <body>
        <%- include('partials/navbar') %>
        <div class="main" id="content">
        <table>
<% books.forEach(s=> 
    { %> <tr>
        <td>
            <a href="/<%= s.ID %>"><img src="../<%= s.image %>" width="250" height="350"></a></td>
        <td><h2><%= s.name %></h2></td>
        </tr>
    <% });  
%>
</table>
</div>

</body>
</html>